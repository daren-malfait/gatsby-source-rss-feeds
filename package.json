{
  "name": "gatsby-source-rss-feeds",
  "version": "0.0.0-semantically-released",
  "description": "gatsby source parser for rss feeds",
  "main": "gatsby-node.js",
  "types": "gatsby-node.d.ts",
  "keywords": [
    "gatsby",
    "source",
    "rss"
  ],
  "author": "Daren Malfait <me@daren.be> (https://daren.be)",
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/daren-malfait/gatsby-source-rss-feeds"
  },
  "bugs": {
    "url": "https://github.com/daren-malfait/gatsby-source-rss-feeds/issues"
  },
  "homepage": "https://github.com/daren-malfait/gatsby-source-rss-feeds#readme",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "cd test && gatsby develop",
    "build": "tsc --outDir .",
    "typecheck": "tsc --noEmit",
